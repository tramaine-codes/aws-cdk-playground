{
  "name": "aws-cdk-playground",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "aws-cdk-playground": "bin/aws-cdk-playground.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "del-cli lib coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.24.0",
    "@release-it/conventional-changelog": "10.0.0",
    "@types/aws-lambda": "8.10.148",
    "@types/node": "22.14.0",
    "@vitest/coverage-v8": "3.1.1",
    "aws-cdk": "2.1007.0",
    "del-cli": "6.0.0",
    "esbuild": "0.25.2",
    "eslint": "9.24.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "release-it": "18.1.2",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vitest": "3.1.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.782.0",
    "@aws-sdk/client-ec2": "3.783.0",
    "@aws-sdk/client-s3": "3.782.0",
    "@aws-sdk/credential-provider-node": "3.782.0",
    "@aws-sdk/lib-dynamodb": "3.782.0",
    "@opensearch-project/opensearch": "2.13.0",
    "@sindresorhus/is": "7.0.1",
    "aws-cdk-lib": "2.188.0",
    "constructs": "10.4.2",
    "date-fns": "4.1.0",
    "dotenv": "16.4.7",
    "env-var": "7.5.0",
    "got": "14.4.7",
    "nanoid": "5.1.5",
    "pkg-dir": "8.0.0",
    "purify-ts": "2.1.0",
    "remeda": "2.21.2",
    "source-map-support": "0.5.21",
    "ts-pattern": "5.7.0"
  }
}
